<!DOCTYPE mapper     PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.exampleliqubase.dao.PaymentMapper" >
    <resultMap id="payment" type="com.example.exampleliqubase.model.PaymentEntity" >
        <id property="id" column="id"/>
        <result property="summa" column="summa"/>
    </resultMap>
    <select id="getPaymentsByIdSub" resultMap="payment" parameterType="java.lang.Integer">
        select * from payments where ref_subscriber = #{id}
    </select>
</mapper>

